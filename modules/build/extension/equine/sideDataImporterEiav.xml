<textFilePopulator>
    <columnDelimiterRegex>\t</columnDelimiterRegex>

	<textFileColumn>
		<identifier>true</identifier>
		<header>sequenceID</header>
		<property>sequenceID</property>
	</textFileColumn>
	
	<textFileColumn>
		<header>pubmed_id</header>
		<property>pubmed_id</property>
		<nullRegex>NK</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull> <!-- will not update to NULL --> 		
	</textFileColumn>

	<textFileColumn>
		<header>species</header>
		<property>isolate_data.host_sci_name</property>
		<nullRegex>NK</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>		
	</textFileColumn>
	
	<textFileColumn>
		<header>subgenus</header>
		<property>subgenus</property>
		<nullRegex>NK</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>		
	</textFileColumn>
	
	<textFileColumn>
		<header>genotype</header>
		<property>genotype</property>
		<nullRegex>NK</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>		
	</textFileColumn>
			
	<textFileColumn>
		<header>country</header>
		<property>isolate_data.country</property>
		<nullRegex>NK</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	
	<textFileColumn>
		<header>collection_year</header>
		<property>isolate_data.collection_year</property>
		<nullRegex>NK</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull> <!-- will not update to NULL --> 		
	</textFileColumn>


</textFilePopulator>
